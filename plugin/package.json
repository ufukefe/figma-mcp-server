{
  "name": "react",
  "version": "1.0.0",
  "type": "module",
  "description": "react",
  "scripts": {
    "test": "npm run tsc && npm run build",
    "format": "prettier --write .",
    "tsc": "npm run tsc:main && npm run tsc:ui",
    "tsc:main": "tsc --noEmit -p main",
    "tsc:ui": "tsc --noEmit -p ui",
    "tsc:watch": "concurrently -n main,ui \"npm run tsc:main -- --watch --preserveWatchOutput\" \"npm run tsc:ui -- --watch --preserveWatchOutput\"",
    "build": "npm run build:ui && npm run build:main -- --minify",
    "build:main": "node esbuild.config.mjs",
    "build:ui": "npx vite build --minify esbuild --emptyOutDir=false",
    "build:watch": "concurrently -n main,ui \"npm run build:main -- --watch\" \"npm run build:ui -- --watch\"",
    "dev": "concurrently -n tsc,build 'npm:tsc:watch' 'npm:build:watch'"
  },
  "author": "Figma",
  "license": "MIT License",
  "dependencies": {
    "@create-figma-plugin/utilities": "^4.0.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "esbuild": "^0.25.12",
    "react": "^19.0.0",
    "react-dom": "^19.2.0",
    "sass": "^1.93.3",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "*",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "concurrently": "^9.2.1",
    "typescript": "^5.9.2",
    "vite": "7.2.1",
    "vite-plugin-singlefile": "^2.3.0",
    "vite-svg-loader": "^5.1.0"
  }
}
